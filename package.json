{
	"name": "mmstudio",
	"version": "1.0.33",
	"description": "Tools for mmstudio development",
	"publisher": "mm",
	"icon": "images/mmstudio.jpeg",
	"galleryBanner": {
		"color": "#005BAC",
		"theme": "light"
	},
	"scripts": {
		"test": "tsc -w",
		"lint": "eslint ./src/**/*.ts",
		"clean": "del-cli ./dist/",
		"compile": "tsc",
		"build": "npm run clean && npm run lint && npm run compile",
		"up": "git pull git@gitee.com:mmstudio/base.git cjs",
		"vscode:prepublish": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && npm run build"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"main": "./dist/index.js",
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "mm.page.add",
				"title": "%mm.page.add%"
			},
			{
				"command": "mm.component.add",
				"title": "%mm.component.add%"
			},
			{
				"command": "mm.action.add",
				"title": "%mm.action.add%"
			},
			{
				"command": "mm.presentation.add",
				"title": "%mm.presentation.add%"
			},
			{
				"command": "mm.service.add",
				"title": "%mm.service.add%"
			},
			{
				"command": "mm.service.router",
				"title": "%mm.service.router%"
			},
			{
				"command": "mm.service.filter",
				"title": "%mm.service.filter%"
			},
			{
				"command": "mm.service.schedule",
				"title": "%mm.service.schedule%"
			},
			{
				"command": "mm.tpl.atom",
				"title": "%mm.tpl.atom%"
			},
			{
				"command": "mm.tpl.tpl",
				"title": "%mm.tpl.tpl%"
			},
			{
				"command": "mm.tpl.widget",
				"title": "%mm.tpl.widget%"
			},
			{
				"command": "mm.shell.create",
				"title": "%mm.shell.create%"
			},
			{
				"command": "mm.shell.debug",
				"title": "%mm.shell.debug%"
			},
			{
				"command": "mm.shell.build",
				"title": "%mm.shell.build%"
			},
			{
				"command": "mm.atom.add",
				"title": "%mm.atom.add%"
			},
			{
				"command": "mm.widget.add",
				"title": "%mm.widget.add%"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "mm.page.add",
					"group": "mm"
				},
				{
					"command": "mm.action.add",
					"when": "editorIsOpen",
					"group": "mm"
				},
				{
					"command": "mm.presentation.add",
					"when": "resourceFilename == tpl.tpl || resourceFilename =~ /p\\d+\\.tpl/",
					"group": "mm"
				},
				{
					"command": "mm.service.add",
					"when": "editorIsOpen",
					"group": "mm"
				},
				{
					"command": "mm.service.router",
					"group": "mm"
				},
				{
					"command": "mm.service.filter",
					"group": "mm"
				},
				{
					"command": "mm.service.schedule",
					"group": "mm"
				},
				{
					"command": "mm.tpl.atom",
					"when": "resourceFilename =~ /a\\d+\\.ts/ || resourceFilename =~ /s\\d+\\.ts/ || resourceFilename == index.ts",
					"group": "mm"
				},
				{
					"command": "mm.tpl.tpl",
					"group": "mm"
				},
				{
					"command": "mm.tpl.widget",
					"when": "resourceFilename =~ /(index|tpl|p\\d+)\\.(tsx?|tpl)/ || resourceFilename =~ /\\w*\\.wxml/",
					"group": "mm"
				},
				{
					"command": "mm.component.add",
					"when": "(config.mm.proj.type == web/h5 && resourceFilename =~ /html\\.ts/ && editorTextFocus) || config.mm.proj.type != web/h5",
					"group": "mm"
				},
				{
					"command": "mm.shell.create",
					"group": "mm"
				},
				{
					"command": "mm.shell.debug",
					"group": "mm"
				},
				{
					"command": "mm.shell.build",
					"group": "mm"
				},
				{
					"command": "mm.atom.add",
					"group": "mm"
				},
				{
					"command": "mm.widget.add",
					"group": "mm"
				}
			],
			"editor/context": [
				{
					"command": "mm.action.add",
					"when": "resourceFilename == b.ts || resourceFilename == p.ts || resourceFilename == n.ts",
					"group": "mm"
				},
				{
					"command": "mm.service.add",
					"when": "editorIsOpen",
					"group": "mm"
				},
				{
					"command": "mm.service.router",
					"group": "mm"
				},
				{
					"command": "mm.service.filter",
					"group": "mm"
				},
				{
					"command": "mm.service.schedule",
					"group": "mm"
				},
				{
					"command": "mm.presentation.add",
					"when": "resourceFilename == tpl.tpl || resourceFilename =~ /p\\d+\\.tpl/",
					"group": "mm"
				},
				{
					"command": "mm.tpl.atom",
					"when": "resourceFilename =~ /a\\d+\\.ts/ || resourceFilename =~ /s\\d+\\.ts/ || resourceFilename == index.ts",
					"group": "mm"
				},
				{
					"command": "mm.tpl.tpl",
					"group": "mm"
				},
				{
					"command": "mm.tpl.widget",
					"when": "resourceFilename =~ /(index|tpl|p\\d+)\\.(tsx?|tpl)/ || resourceFilename =~ /\\w*\\.wxml/",
					"group": "mm"
				}
			],
			"explorer/context": [
				{
					"command": "mm.page.add",
					"group": "mm"
				}
			]
		},
		"snippets": [
			{
				"language": "typescript",
				"path": "./snippets/html.json"
			},
			{
				"language": "html",
				"path": "./snippets/html.json"
			}
		],
		"configuration": [
			{
				"title": "项目类型",
				"properties": {
					"mm.proj.type": {
						"type": "string",
						"enum": [
							"web/h5",
							"wxapp",
							"desktop",
							"mobile"
						],
						"default": "web/h5",
						"description": "%mm.proj.type%"
					},
					"mm.atom.type": {
						"type": "string",
						"enum": [
							"nodejs",
							"web/h5",
							"wxapp",
							"desktop",
							"mobile"
						],
						"default": "web/h5",
						"description": "%mm.atom.type%"
					},
					"mm.widget.type": {
						"type": "string",
						"enum": [
							"web/h5",
							"wxapp",
							"desktop",
							"mobile"
						],
						"default": "web/h5",
						"description": "%mm.widget.type%"
					}
				}
			}
		],
		"keybindings": [
			{
				"command": "mm.page.add",
				"key": "alt+p"
			},
			{
				"command": "mm.component.add",
				"when": "(config.mm.proj.type == web/h5 && resourceFilename =~ /html\\.ts/ && editorTextFocus) || config.mm.proj.type != web/h5",
				"key": "alt+c"
			},
			{
				"command": "mm.action.add",
				"when": "editorIsOpen",
				"key": "alt+a"
			},
			{
				"command": "mm.service.add",
				"when": "editorIsOpen",
				"key": "alt+s"
			},
			{
				"command": "mm.service.router",
				"key": "alt+r"
			},
			{
				"command": "mm.service.filter",
				"key": "alt+m f"
			},
			{
				"command": "mm.service.schedule",
				"key": "alt+m s"
			},
			{
				"command": "mm.presentation.add",
				"when": "resourceFilename == tpl.tpl || resourceFilename =~ /p\\d+\\.tpl/",
				"key": "alt+x"
			},
			{
				"command": "mm.tpl.atom",
				"when": "resourceFilename =~ /a\\d+\\.ts/ || resourceFilename =~ /s\\d+\\.ts/ || resourceFilename == index.ts",
				"key": "alt+t a"
			},
			{
				"command": "mm.tpl.tpl",
				"key": "alt+t t"
			},
			{
				"command": "mm.tpl.widget",
				"when": "resourceFilename =~ /(index|tpl|p\\d+)\\.(tsx?|tpl)/ || resourceFilename =~ /\\w*\\.wxml/",
				"key": "alt+t w"
			},
			{
				"command": "mm.shell.create",
				"key": "alt+m c"
			},
			{
				"command": "mm.shell.debug",
				"key": "alt+m d"
			},
			{
				"command": "mm.shell.build",
				"key": "alt+m b"
			},
			{
				"command": "mm.atom.build",
				"key": "alt+m a"
			},
			{
				"command": "mm.widget.build",
				"key": "alt+m w"
			}
		]
	},
	"keywords": [
		"蛮蛮",
		"蛮蛮工作室",
		"框架",
		"framework",
		"web",
		"factory",
		"tools",
		"mm",
		"mmstudio"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/mm-core/tools"
	},
	"homepage": "https://mmstudio.gitee.io/",
	"author": {
		"name": "taoqf",
		"email": "tao_qiufeng@126.com",
		"url": "https://mmstudio.gitee.io/"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "13.7.7",
		"@types/node-fetch": "2.5.5",
		"@types/vscode": "1.37.0",
		"@typescript-eslint/eslint-plugin": "2.22.0",
		"@typescript-eslint/parser": "2.22.0",
		"cp-cli": "2.0.0",
		"del-cli": "3.0.0",
		"eslint": "6.8.0",
		"eslint-config-prettier": "6.10.0",
		"eslint-plugin-import": "2.20.1",
		"typescript": "next",
		"vscode-test": "1.3.0"
	}
}
