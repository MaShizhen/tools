		"menus": {
			"commandPalette": [
				{
					"command": "mm.transfiles",
					"group": "mmstudio"
				},
				{
					"command": "mm.generatetable",
					"group": "mmstudio"
				},
				{
					"command": "mm.prototype",
					"group": "mmstudio"
				},
				{
