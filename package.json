				},
				{
					"command": "mm.shell.create",
					"group": "mmstudio"
				},
				{
					"command": "mm.shell.dev",
					"group": "mmstudio"
				},
				{
					"command": "mm.shell.build",
					"group": "mmstudio"
				},
				{
					"command": "mm.atom.add",
					"group": "mmstudio"
				},
				{
					"command": "mm.atom.addlocal",
					"group": "mmstudio"
				},
				{
					"command": "mm.doc.find",
					"group": "mmstudio"
				},
				{
					"command": "mm.widget.add",
					"group": "mmstudio"
				},
				{
					"command": "mm.widget.addlocal",
					"group": "mmstudio"
				},
				{
					"command": "mm.html2jsx",
					"when": "editorHasSelection",
					"group": "mmstudio"
				}
			],
			"editor/context": [
				{
					"command": "mm.transfiles",
					"group": "mmstudio"
				},
				{
					"command": "mm.generatetable",
					"group": "mmstudio"
				},
				{
					"command": "mm.prototype",
					"group": "mmstudio"
				},
				{
					"command": "mm.service.add",
					"when": "editorIsOpen",
					"group": "mmstudio"
				},
				{
					"command": "mm.service.schedule",
					"group": "mmstudio"
				},
				{
					"command": "mm.tpl.atom",
					"group": "mmstudio"
				},
				{
					"command": "mm.tpl.tpl",
					"group": "mmstudio"
				},
				{
					"command": "mm.tpl.widget",
					"group": "mmstudio"
				},
				{
					"command": "mm.html2jsx",
					"when": "editorHasSelection && textInputFocus",
					"group": "mmstudio"
				}
			],
			"explorer/context": [
				{
					"command": "mm.service.add",
					"group": "mmstudio"
				},
				{
					"command": "mm.page.add",
					"group": "mmstudio"
				},
				{
					"command": "mm.component.add2",
					"group": "mmstudio"
				},
				{
					"command": "mm.atom.addlocal2",
					"group": "mmstudio"
				}
			]
		},
		"snippets": [
			{
				"language": "typescriptreact",
				"path": "./snippets/tsx.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/ts.json"
			}
		],
		"configuration": [
			{
				"title": "项目类型",
				"properties": {
					"mm.proj.type": {
						"type": "string",
						"enum": [
							"next",
							"taro",
							"uniapp",
							"desktop"
						],
						"description": "%mm.proj.type%"
					}
				}
			}
		],
		"keybindings": [
			{
				"command": "mm.transfiles",
				"key": "alt+m alt+f"
			},
			{
				"command": "mm.generatetable",
				"key": "alt+m t"
			},
			{
				"command": "mm.prototype",
				"key": "alt+i"
			},
			{
				"command": "mm.page.add",
				"key": "alt+p"
			},
			{
				"command": "mm.component.add",
				"key": "alt+c"
			},
			{
				"command": "mm.service.add",
				"key": "alt+s"
			},
			{
				"command": "mm.tpl.atom",
				"key": "alt+t a"
			},
			{
				"command": "mm.tpl.tpl",
				"key": "alt+t t"
			},
			{
				"command": "mm.tpl.widget",
				"key": "alt+t w"
			},
			{
				"command": "mm.service.schedule",
				"key": "alt+m s"
			},
			{
				"command": "mm.shell.create",
				"key": "alt+m p"
			},
			{
				"command": "mm.shell.dev",
				"key": "alt+m d"
			},
			{
				"command": "mm.shell.build",
				"key": "alt+m b"
			},
			{
				"command": "mm.atom.add",
				"key": "alt+m alt+a"
			},
			{
				"command": "mm.atom.addlocal",
				"key": "alt+a"
			},
			{
				"command": "mm.doc.find",
				"key": "alt+m f"
			},
			{
				"command": "mm.widget.add",
				"key": "alt+m alt+w"
			},
			{
				"command": "mm.widget.addlocal",
				"key": "alt+m w"
			},
			{
				"command": "mm.html2jsx",
				"when": "editorHasSelection && textInputFocus",
				"key": "alt+j"
			},
			{
				"command": "mm.linebreak",
				"key": "alt+n"
			},
			{
				"command": "mm.regenerateapis",
				"key": "alt+m+a"
			},
			{
				"command": "mm.regeneratepages",
				"key": "alt+m+p"
			}
		]
